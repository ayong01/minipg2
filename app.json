{
  "pages":[
    "pages/index/index",
    "pages/logs/logs",
    "pages/category/category",
    "pages/car/car",
    "pages/my/my"
  ],
  "window":{
    "backgroundTextStyle":"light",
    "navigationBarBackgroundColor": "#FFFFFF",
    "navigationBarTitleText": "购物街",
    "navigationBarTextStyle":"black",
    "enablePullDownRefresh":true,
    "backgroundTextStyle": "light",
    "backgroundColor":"#1DB34C",
    "backgroundColorTop":"#5093FE",
    "onReachBottomDistance":50,
    "pageOrientation":"portrait",
    "pageOrientation": "auto"
  },
  "tabBar":{
    "color":"#6e6d6b",
    "backgroundColor":"#FFFFFF",
    "selectedColor":"#FF26FF",
    "borderStyle":"black",
    "position":"bottom",
    "list": [
      {"pagePath":"pages/index/index",
        "text":"首页",
        "iconPath":"images/nav/home-off.png",
        "selectedIconPath":"images/nav/home-on.png"},
        {"pagePath":"pages/category/category",
          "text":"分类",
          "iconPath":"images/nav/ic_catefory_normal.png",
          "selectedIconPath":"images/nav/ic_catefory_pressed.png"},
          {"pagePath":"pages/car/car",
            "text":"购物车",
            "iconPath":"images/nav/cart-off.png",
            "selectedIconPath":"images/nav/order-on.png"},
            {"pagePath":"pages/my/my",
              "text":"我的",
              "iconPath":"images/nav/my-off.png",
              "selectedIconPath":"images/nav/my-on.png"}
    ]
  },
  "style": "v2",
  "sitemapLocation": "sitemap.json",
  "darkmode": false,
  "lazyCodeLoading": "requiredComponents",
  "debug": true,
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    }
  }
}
